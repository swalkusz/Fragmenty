<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#A64CAF50"
    tools:context=".WyjscieFragment">

    <!-- TODO: Update blank fragment layout -->
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent">


        <TextView
            android:id="@+id/oldChainTitle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/oldChain" />

        <TextView
            android:id="@+id/oldChain"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/sortedChainTitle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/sortedChain" />

        <TextView
            android:id="@+id/sortedChain"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />
    </LinearLayout>

</FrameLayout>